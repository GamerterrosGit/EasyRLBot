{
  "name": "easyrlbot",
  "version": "0.4.2",
  "description": "A RLBot implementation in javascript with Sockets.",
  "main": "dist/index",
  "scripts": {
    "watch": "npx tsc --watch",
    "compile": "tsc && npm run closure",
    "closure": "npx google-closure-compiler --js=\"dist/**.js\" --js_output_file=dist/small.js --language_out=ECMASCRIPT3 -O=BUNDLE"
  },
  "keywords": [
    "rl",
    "bot",
    "rlbot",
    "rocket",
    "league",
    "rocketleague",
    "easy",
    "typescript",
    "ts"
  ],
  "author": "Simon Lindgren",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "flatbuffers": "^1.12.0"
  },
  "devDependencies": {
    "@types/flatbuffers": "^1.10.0",
    "google-closure-compiler": "^20210202.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
