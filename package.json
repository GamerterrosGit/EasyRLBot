{
  "name": "easyrlbot",
  "version": "0.5.4",
  "description": "A RLBot implementation in javascript with Sockets.",
  "main": "dist/index",
  "scripts": {
    "watch": "npx tsc --watch",
    "build": "npm run build:typecheck && npm run build:js",
    "build:js": "node esbuild.js",
    "build:typecheck": "tsc",
    "flatc": "cd src/flat && flatc --gen-object-api --ts rlbot.fbs"
  },
  "keywords": [
    "rl",
    "bot",
    "rlbot",
    "rocket",
    "league",
    "rocketleague",
    "easy",
    "typescript",
    "ts"
  ],
  "author": "Simon Lindgren",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "flatbuffers": "^2.0.7"
  },
  "devDependencies": {
    "@types/flatbuffers": "^1.10.0",
    "esbuild": "^0.15.5",
    "typescript": "^4.1.3"
  }
}
